  CHARACTER SET UTF8
  FIELDS TERMINATED BY ',' ENCLOSED BY '"'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;
LOAD DATA LOCAL INFILE '/root/mysql/schema/sample/subject.data' INTO TABLE ustudy.subject
  CHARACTER SET UTF8
  FIELDS TERMINATED BY ',' ENCLOSED BY '"'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;
LOAD DATA LOCAL INFILE '/root/mysql/schema/sample/rolevalue.data' INTO TABLE ustudy.rolevalue
  CHARACTER SET UTF8
  FIELDS TERMINATED BY ',' ENCLOSED BY '"'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;
LOAD DATA LOCAL INFILE '/root/mysql/schema/sample/config.data' REPLACE INTO TABLE ustudy.config
  CHARACTER SET UTF8
  FIELDS TERMINATED BY ',' ENCLOSED BY '$'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;

