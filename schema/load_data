LOAD DATA LOCAL INFILE '/root/mysql/schema/sample/users.data' INTO TABLE ustudy.users
  CHARACTER SET UTF8
  FIELDS TERMINATED BY ',' ENCLOSED BY '"'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;
LOAD DATA LOCAL INFILE '/root/mysql/schema/sample/roles.data' INTO TABLE ustudy.roles
  CHARACTER SET UTF8
  FIELDS TERMINATED BY ',' ENCLOSED BY '"'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;
